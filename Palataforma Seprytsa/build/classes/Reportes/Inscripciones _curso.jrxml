	<queryString>
		<![CDATA[SELECT 
estudiante.cod_est,
estudiante.Apellido_est,
estudiante.Nombre_est

FROM 
estudiante,
cursos,
cursos_estudiante
WHERE 
cursos_estudiante.cod_est= estudiante.cod_est
AND cursos_estudiante.codigo_cur= cursos.codigo_cur
AND cursos_estudiante.Anio= $P{anio}
AND cursos_estudiante.codigo_cur=$P{entidad}
ORDER BY estudiante.Apellido_est]]>
